<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="klinka.com.notciasagora">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_news_now"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_news_now_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="klinka.com.newsnow.ui.activity.DetailsNewsActivity"></activity>
        <activity android:name="klinka.com.newsnow.ui.activity.ListNewsActivity" />
        <activity android:name="klinka.com.newsnow.ui.activity.NewsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>